{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite client --config vite.config.ts",
    "build": "tsc -b && vite build",
    "test": "echo TODO",
    "lint": "yarn lint:types && yarn lint:style && yarn lint:format",
    "lint:types": "tsc --noEmit",
    "lint:style": "eslint client/**/*.{ts,tsx} --max-warnings 0 --report-unused-disable-directives",
    "lint:format": "prettier --list-different \"client/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "gql:gen": "graphql-codegen --config client/codegen.ts",
    "wait-on-server": "wait-on http://localhost:30001 -t 1m"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@apollo/client": "^3.11.8",
    "antd": "^5.20.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "ts-deepmerge": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.1",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "graphql": "^16.9.0",
    "prettier": "^3.3.3",
    "typescript": "<5.6.0",
    "vite": "^5.4.7",
    "wait-on": "^8.0.1"
  }
}
