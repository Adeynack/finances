{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite client --config vite.config.ts",
    "build": "tsc -b && vite build",
    "lint": "yarn lint:types && yarn lint:style && yarn lint:format",
    "lint:types": "tsc --noEmit",
    "lint:style": "eslint client/**/*.ts --max-warnings 0 --report-unused-disable-directives",
    "lint:format": "prettier --list-different \"client/**/*.ts\"",
    "preview": "vite preview",
    "gql-compile": "graphql-codegen --config client/codegen.ts",
    "gql-watch": "graphql-codegen  --config client/codegen.ts --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@apollo/client": "^3.10.8",
    "antd": "^5.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.9.0",
    "graphql": "^16.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.2",
    "vite": "^5.2.13",
    "wait-on": "^8.0.0"
  }
}
